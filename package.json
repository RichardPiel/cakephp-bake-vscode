{
	"name": "cakephp-bake",
	"publisher": "RichardPiel",
	"displayName": "CakePHP Bake",
	"description": "Quickly bake models, templates, controllers and migrations in CakePHP 3/4",
	"icon": "images/cakephp.png",
	"keywords": ["php", "framework", "cakephp", "bake", "model", "table", "entity", "migration", "controller", "prefix", "plugin", "php"],
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://RPL0930@dev.azure.com/RPL0930/CakePHP%20Bake/_git/CakePHP%20Bake"
	},
	"activationEvents": [
		"onCommand:cakephp-bake.bakeController",
		"onCommand:cakephp-bake.bakeModel",
		"onCommand:cakephp-bake.bakeTemplate",
		"onCommand:cakephp-bake.bakeMigration",
		"onCommand:cakephp-bake.MigrationsMigrate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cakephp-bake.bakeController",
				"title": "CakePHP Bake: Controller"
			},
			{
				"command": "cakephp-bake.bakeModel",
				"title": "CakePHP Bake: Model"
			},
			{
				"command": "cakephp-bake.bakeTemplate",
				"title": "CakePHP Bake: Template"
			},
			{
				"command": "cakephp-bake.bakeMigration",
				"title": "CakePHP Bake: Migration"
			},
			{
				"command": "cakephp-bake.MigrationsMigrate",
				"title": "CakePHP Migrations: Migrate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.20.4",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
