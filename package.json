{
	"name": "cakephp-bake",
	"publisher": "RichardPiel",
	"displayName": "CakePHP Bake",
	"description": "Quickly bake models, templates, controllers and migrations in CakePHP 3/4",
	"icon": "images/cakephp.png",
	"keywords": [
		"php",
		"framework",
		"cakephp",
		"bake",
		"model",
		"table",
		"entity",
		"migration",
		"controller",
		"prefix",
		"plugin",
		"php",
		"cell",
		"helper",
		"shell",
		"command"
	],
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://RPL0930@dev.azure.com/RPL0930/CakePHP%20Bake/_git/CakePHP%20Bake"
	},
	"activationEvents": [
		"onCommand:cakephp-bake.bakeController",
		"onCommand:cakephp-bake.bakeModel",
		"onCommand:cakephp-bake.bakeTemplate",
		"onCommand:cakephp-bake.bakeMigration",
		"onCommand:cakephp-bake.bakeCell",
		"onCommand:cakephp-bake.bakeHelper",
		"onCommand:cakephp-bake.bakePlugin",
		"onCommand:cakephp-bake.bakeCommand",
		"onCommand:cakephp-bake.bakeFixture",
		"onCommand:cakephp-bake.bakeShell",
		"onCommand:cakephp-bake.MigrationsMigrate",
		"onCommand:cakephp-bake.bakeComponent",
		"onCommand:cakephp-bake.bakeBehavior",
		"onCommand:cakephp-bake.bakeMailer",
		"onCommand:cakephp-bake.bakeTask",
		"onCommand:cakephp-bake.bakeMiddleware"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cakephp-bake.bakeBehavior",
				"title": "CakePHP Bake: Behavior"
			},
			{
				"command": "cakephp-bake.bakeFixture",
				"title": "CakePHP Bake: Fixture"
			},
			{
				"command": "cakephp-bake.bakeMailer",
				"title": "CakePHP Bake: Mailer"
			},
			{
				"command": "cakephp-bake.bakeMiddleware",
				"title": "CakePHP Bake: Middleware"
			},
			{
				"command": "cakephp-bake.bakeController",
				"title": "CakePHP Bake: Controller"
			},
			{
				"command": "cakephp-bake.bakeModel",
				"title": "CakePHP Bake: Model"
			},
			{
				"command": "cakephp-bake.bakeTemplate",
				"title": "CakePHP Bake: Template"
			},
			{
				"command": "cakephp-bake.bakeMigration",
				"title": "CakePHP Bake: Migration"
			},
			{
				"command": "cakephp-bake.MigrationsMigrate",
				"title": "CakePHP Migrations: Migrate"
			},
			{
				"command": "cakephp-bake.bakeCommand",
				"title": "CakePHP Bake: Command"
			},
			{
				"command": "cakephp-bake.bakeHelper",
				"title": "CakePHP Bake: Helper"
			},
			{
				"command": "cakephp-bake.bakeCell",
				"title": "CakePHP Bake: Cell"
			},
			{
				"command": "cakephp-bake.bakeShell",
				"title": "CakePHP Bake: Shell"
			},
			{
				"command": "cakephp-bake.bakePlugin",
				"title": "CakePHP Bake: Plugin"
			},
			{
				"command": "cakephp-bake.bakeTask",
				"title": "CakePHP Bake: Task"
			},
			{
				"command": "cakephp-bake.bakeComponent",
				"title": "CakePHP Bake: Component"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.20.4",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"strip-ansi": "^6.0.0"
	}
}
